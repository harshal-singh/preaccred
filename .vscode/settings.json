{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "cSpell.words": [
    "appspot",
    "compat",
    "Criterias",
    "datepicker",
    "firebaseapp",
    "fluentui",
    "hasura",
    "pipesort",
    "preaccred",
    "tanstack"
  ],
  "typescript.tsdk": "frontend\\node_modules\\typescript\\lib"
}
